{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
     {
      "name": "Cortex Debug",
      "cwd": "${workspaceRoot}",
      "executable": "${command:cmake.launchTargetPath}",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "serverpath": "JLinkGDBServer",
      "gdbPath": "gdb-multiarch",
      "device": "RP2350_M33_0",
      "svdFile": "${env:PICO_SDK_PATH}/src/rp2350/hardware_regs/RP2350.svd",
      "runToEntryPoint": "main",
      // Give restart the same functionality as runToEntryPoint - main
      "postRestartCommands": [
          "break main",
          "continue"
      ],
      "postLaunchCommands": [
        "monitor semihosting enable",
        "monitor semihosting ioclient 3", // 1: telnet (port 2333); 2: gdb; 3: both telnet and gdbclient output
      ],
      "rttConfig": {
          "enabled": true,
          // "clearSearch": false    // OpenOCD users may have to un-comment this
          "address": "auto",
          "decoders": [
              {
                  "type": "console",
                  "port": 0,
                  "encoding": "unsigned",
                  "graphId": "1",
                  "scale": 1
              }
          ]
      },
      "showDevDebugOutput": "none",
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      }
    },
  ]
}